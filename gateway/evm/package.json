    "@ethersproject/providers": "^5.4.3",
    "@matterlabs/hardhat-zksync-deploy": "^1.0.1",
    "@matterlabs/hardhat-zksync-upgradable": "^1.1.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
